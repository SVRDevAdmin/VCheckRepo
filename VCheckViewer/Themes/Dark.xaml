<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <SolidColorBrush Color="#1f2e45"  x:Key="FrameBackground"></SolidColorBrush>
    <SolidColorBrush Color="#172232" x:Key="PageBackground"></SolidColorBrush>
    <SolidColorBrush Color="White" x:Key="LoginPageFontColor"></SolidColorBrush>

    <!-- Login -->
    <BitmapImage UriSource="../../../Content/Images/bio_logo_dark.png" x:Key="BioLogo"></BitmapImage>
    <BitmapImage UriSource="../../../Content/Images/paw_dark.png" x:Key="PawImage"></BitmapImage>
    <BitmapImage UriSource="../../../Content/Images/BackArrowRound_white.png" x:Key="Themes_BackArrowRoundButton"></BitmapImage>

    <!-- Master -->
    <SolidColorBrush Color="#1f2e45" x:Key="MasterPageFrameBackground"></SolidColorBrush>
    <SolidColorBrush Color="White" x:Key="MasterPageFontColor"></SolidColorBrush>
    <SolidColorBrush Color="#172232" x:Key="Themes_MenuHighligted"></SolidColorBrush>
    <SolidColorBrush Color="White" x:Key="Themes_FontColor"></SolidColorBrush>
    <SolidColorBrush Color="#1e76fb" x:Key="Themes_ButtonColor"></SolidColorBrush>
    <SolidColorBrush Color="white" x:Key="Themes_ButtonFontColor"></SolidColorBrush>
    <SolidColorBrush Color="#1f2e45" x:Key="Themes_PopupMessageFrameBackground"></SolidColorBrush>
    <SolidColorBrush Color="White" x:Key="Themes_PopupMessageFontColor"></SolidColorBrush>
    <BitmapImage UriSource="../../Content/Images/bio_logo_dark.png" x:Key="TemplateBioLogo"></BitmapImage>
